pipeline {

  environment {
    dockerimagebackend = "davidsmenesesc/rampup-back:v1"
    dockerImage = ""
    dockerimagefrontend = "davidsmenesesc/rampup-front:v1"
  }

agent any

stages {

    stage('Checkout Source') {
      steps {
        git 'https://github.com/davidsmenesesc/rampup.git'
      }
    }

    stage('Build image') {
      steps{
        script {
          dockerImage = docker.build dockerimagename
        }
      }
    }
 }
}