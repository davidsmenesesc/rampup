pipeline {

  environment {
    dockerimagebackend = "movie-analyst-api/Dockerfile"
    dockerImage = ""
    dockerimagefrontend = "movie-analyst-ui/Dockerfile"
  }

agent any

stages {

    stage('Checkout Source') {
      steps {
        git 'https://github.com/davidsmenesesc/rampup.git'
      }
    }

    stage('Build image') {
      steps{
        script {
          dockerBack = docker.build dockerimagebackend
          dockerFront = docker.build dockerimagefrontend
        }
      }
    }
 }
}