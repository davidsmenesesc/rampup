pipeline {

  environment {
    dockerimagebackend = "davidsmenesesc/rampup-back"
    dockerImage = ""
    dockerimagefrontend = "movie-analyst-ui"
  }

agent any

stages {

    stage('Checkout Source') {
      steps {
        git 'https://github.com/davidsmenesesc/rampup.git'
      }
    }
    stage('Build image') {
      steps{
        script {
        dir('rampup/movie-analyst-api') {
                dockerBack = docker.build dockerimagebackend
            }            

        }
      }
    }
 }
}